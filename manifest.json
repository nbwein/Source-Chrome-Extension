{
	"manifest_version": 2,
	"minimum_chrome_version": "44", 
	"name": "Stella Source", 
	"description": "StellaService Internal Homepage", 
	"version": "1.5",
	"chrome_url_overrides": {
   		 "newtab": "main.html"
  	},
        "permissions": ["<all_urls>", "geolocation", "api.hipchat.com/v2/oauth/token", "gcm", "storage", "webRequest", "webRequestBlocking", "*://*.google.com/", "identity", "https://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js", "https://accounts.google.com/*", "https://www.googleapis.com/*", "https://www.googleapis.com/plusDomains/v1/people/me"], 
	"oauth2": {
		"client_id": "xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx.apps.googleusercontent.com",
		"scopes": ["https://www.googleapis.com/auth/calendar", "https://www.googleapis.com/auth/plus.me", "https://www.googleapis.com/auth/userinfo.email"]
	},
	"content_security_policy": "script-src 'self' https://stellaservice.atlassian.net/xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx https://ssl.google-analytics.com https://www.google.com/uds/ https://www.google.com/jsapi https://apis.google.com/js/client.js https://apis.google.com; object-src 'self'"

}
